#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5ffef6301490 .scope module, "tb_button" "tb_button" 2 3;
 .timescale -9 -12;
v0x5ffef6354650_0 .var "clk", 0 0;
v0x5ffef6354710_0 .var "pb_1", 0 0;
v0x5ffef63547e0_0 .net "pb_out", 0 0, v0x5ffef6354400_0;  1 drivers
S_0x5ffef6301620 .scope module, "uut" "keyDebouncing" 2 3, 3 1 0, S_0x5ffef6301490;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "keyIn";
    .port_info 3 /OUTPUT 1 "keyOut";
P_0x5ffef63017b0 .param/l "RESET" 1 3 8, C4<1111>;
P_0x5ffef63017f0 .param/l "SET" 1 3 7, C4<0000>;
P_0x5ffef6301830 .param/l "samplingNum" 1 3 6, +C4<00000000000000000000000000000100>;
v0x5ffef633e630_0 .net "clk", 0 0, v0x5ffef6354650_0;  1 drivers
v0x5ffef6354170_0 .var/i "i", 31 0;
v0x5ffef6354250_0 .net "keyIn", 0 0, v0x5ffef6354710_0;  1 drivers
v0x5ffef6354320_0 .var "keyInReg", 3 0;
v0x5ffef6354400_0 .var "keyOut", 0 0;
L_0x7e4712e86018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x5ffef6354510_0 .net "rst", 0 0, L_0x7e4712e86018;  1 drivers
E_0x5ffef633f050 .event posedge, v0x5ffef6354510_0, v0x5ffef633e630_0;
    .scope S_0x5ffef6301620;
T_0 ;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x5ffef6354320_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x5ffef6301620;
T_1 ;
    %wait E_0x5ffef633f050;
    %load/vec4 v0x5ffef6354510_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x5ffef6354320_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5ffef6354250_0;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5ffef6354320_0, 4, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5ffef6354170_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x5ffef6354170_0;
    %cmpi/s 3, 0, 32;
    %jmp/0xz T_1.3, 5;
    %load/vec4 v0x5ffef6354320_0;
    %load/vec4 v0x5ffef6354170_0;
    %part/s 1;
    %ix/load 5, 0, 0;
    %load/vec4 v0x5ffef6354170_0;
    %addi 1, 0, 32;
    %ix/vec4/s 4;
    %assign/vec4/off/d v0x5ffef6354320_0, 4, 5;
    %load/vec4 v0x5ffef6354170_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5ffef6354170_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
T_1.1 ;
    %load/vec4 v0x5ffef6354320_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 4;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5ffef6354400_0, 0;
    %jmp T_1.6;
T_1.4 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5ffef6354400_0, 0;
    %jmp T_1.6;
T_1.6 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5ffef6301490;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354650_0, 0, 1;
T_2.0 ;
    %delay 10000, 0;
    %load/vec4 v0x5ffef6354650_0;
    %inv;
    %store/vec4 v0x5ffef6354650_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5ffef6301490;
T_3 ;
    %vpi_call 2 12 "$dumpfile", "debounce_test.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5ffef6301490 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5ffef6301490;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 30000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 40000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 30000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 1000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 30000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 40000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ffef6354710_0, 0, 1;
    %delay 10000000, 0;
